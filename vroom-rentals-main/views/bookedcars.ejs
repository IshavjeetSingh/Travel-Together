<%- include('partials/header') %>

  <h3>YOUR BOOKED CARS</h3>
<%if(cars.length == 0){%>
  <p>Sorry! You do not have any bookings currently.</p>
<%}%>


  <div class="container" style="margin-top: 150px;">
    <div class="row">
      <%cars.forEach(function(car){%>
      <div class="col-md-3">
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            <h5 class="card-title">Model: <%=car.model%></h5>
            <p class="card-text">Number: <%=car.number%></p>
            <p class="card-text">Rent: INR <%=car.rent%> /day</p>
            <p class="card-text">Capacity: <%=car.capacity%></p>
            <p class="card-text">Status: <%=car.status%></p>
            <p class="card-text">Booked By: <%=car.customer%></p>
            <p class="card-text">Booked From: <%=car.startdate%></p>
            <p class="card-text">Booked For: <%=car.days%> Days</p>
          </div>
        </div>
      </div>
      <%});%>
    </div>
  </div>



<a role="button" class="btn btn-primary" href="/agencydashboard">Dashboard</a>
<%- include('partials/footer') %>
